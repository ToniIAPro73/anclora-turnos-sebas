{
  "name": "shift-import-proxy",
  "version": "1.0.0",
  "description": "Standalone proxy server for extracting work shifts from calendar images using Forge API",
  "main": "proxy-server.ts",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development npx tsx proxy-server.ts",
    "start": "NODE_ENV=production npx tsx proxy-server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "shift",
    "calendar",
    "extraction",
    "vision",
    "llm",
    "forge-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.7.0",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3"
  }
}
